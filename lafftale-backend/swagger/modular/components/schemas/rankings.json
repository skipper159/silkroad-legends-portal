  "JobProgressionAnalytics": {
    "type": "object",
    "description": "Job progression analytics data",
    "properties": {
      "jobType": {
        "type": "integer",
        "enum": [1, 2, 3],
        "description": "Job type analyzed (1=Trader, 2=Thief, 3=Hunter)"
      },
      "timeframe": {
        "type": "string",
        "enum": ["week", "month", "quarter", "year"],
        "description": "Analysis timeframe"
      },
      "totalPlayers": {
        "type": "integer",
        "description": "Total players in this job type"
      },
      "averageLevel": {
        "type": "number",
        "description": "Average job level"
      },
      "levelDistribution": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "level": { "type": "integer" },
            "playerCount": { "type": "integer" },
            "percentage": { "type": "number" }
          }
        },
        "description": "Distribution of players across levels"
      },
      "experienceGrowth": {
        "type": "object",
        "properties": {
          "totalExpGained": { "type": "integer" },
          "averageExpPerPlayer": { "type": "number" },
          "topExpGainer": {
            "type": "object",
            "properties": {
              "CharName16": { "type": "string" },
              "expGained": { "type": "integer" }
            }
          }
        }
      },
      "progressionTrends": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "date": { "type": "string", "format": "date" },
            "newPlayers": { "type": "integer" },
            "levelUps": { "type": "integer" },
            "averageProgress": { "type": "number" }
          }
        },
        "description": "Daily/weekly progression trends"
      }
    },
    "required": [
      "jobType",
      "timeframe",
      "totalPlayers",
      "averageLevel",
      "levelDistribution"
    ]
  }
}
